package test2;

public interface SMSList {
	// 添加学生信息
	void addLast(Student stu);
	// 在第一个位置添加学生信息
	void addFirst(Student stu);
	// 在指定位置添加学生信息
	void add(int index, Student stu);
	// 获取指定位置学生信息
	Student get(int index);
	//获取存放学生的个数
	int size();
	// 替换指定索引位置上的学生
	void set(int index, Student stu);
	// 删除指定索引位置的学生
	Student remove(int index);
	Student removeFirst();
	Student removeLast();
}
