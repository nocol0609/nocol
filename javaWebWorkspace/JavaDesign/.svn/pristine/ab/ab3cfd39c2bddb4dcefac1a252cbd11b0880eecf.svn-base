package com.java;

/**
 * 打印100到1000的水仙花数
 * @author Nocol
 *
 */
public class flowerTest {
	public static void main(String[] args) {
		
		int count=0;
		
		for (int i = 100; i < 1000; i++) {
			int g=i%10;
			int s=i/10%10;
			int b=i/10/10%10;
			
			if(i==(g*g*g)+(s*s*s)+(b*b*b)){
				System.out.println(i);
				count++;
			}
		}
		System.out.println("100到1000的水仙花数有："+count);
	}
}
