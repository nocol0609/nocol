<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssm.dao.PriceRankDao">
	
	<!--查询所有价格区间  -->
	<select id="findAllPriceRank" resultType="PriceRank">
		select dpr.id,dpr.min_price,max_price
		from d_price_rank dpr
	</select>
	
</mapper>