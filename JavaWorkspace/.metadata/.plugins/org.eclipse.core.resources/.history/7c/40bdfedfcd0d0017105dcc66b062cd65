public class CollectionTest {
	public static void main(String[] args) {
		//数组可以存放对象
//		Student[] s = new Student[5];
//		s[5]=new Student();
//		System.out.println(s[5]);
		//set集合无序不可重复
/*		Set<Object> set=new HashSet<Object>();
		set.add(10);
		set.add(10);
		set.add(true);
		set.add(Math.random());
		set.add("Hello");
		//System.out.println(set.size());	
