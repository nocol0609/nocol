public class javaTest {

	/**
	 * 求给定两个数的最大公约数最小公倍数
	 * 
	 */
	@Test
	public void beiyue() {
		Scanner sc = new Scanner(System.in);
		System.out.println("请输入:");
		int m = sc.nextInt();
		int n = sc.nextInt();

		int yue = 1;
		int bei = m * n;
		int temp = 1;

		if (n < m) {
			temp = n;
			n = m;
			m = temp;
		}

		if (m != 0) {
			temp = n % m;
			n = m;
			m = temp;
		}

		yue = n;
		bei /= n;

		System.out.println("yue:" + yue);
		System.out.println("bei:" + bei);
		System.out.println();
	}

